{
  "application": "Lisa Test Environment",
  "version": "1.0.0",
  "description": "Example configuration file stored in resources directory",
  "settings": {
    "database": {
      "host": "localhost",
      "port": 5432,
      "database": "lisa_test",
      "pool_size": 10
    },
    "server": {
      "host": "0.0.0.0",
      "port": 3000,
      "workers": 4
    },
    "logging": {
      "level": "info",
      "output": "/var/log/lisa/app.log"
    },
    "features": {
      "enable_caching": true,
      "enable_metrics": true,
      "enable_tracing": false
    }
  }
}
